{"ts":1369745330039,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"package com.test\n\nclass QueryStringHelper {\n\n    def static asQueryString(Map params) {\n\n        params.collect { k, v ->\n\n            def encodedValue = URLEncoder.encode(v.toString(), 'UTF8')\n            \"${k}=${encodedValue}\"\n\n        }.sort().join(\"&\")\n    }\n}\n"]],"start1":0,"start2":0,"length1":0,"length2":265}]],"length":265}
